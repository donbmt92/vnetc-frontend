{
    "version": 2,
    "routes": [
        { "src": "/api/(.*)", "dest": "/api/json-server.js" },
        { "src": "/(.*)", "dest": "/index.html" }
    ],
    "builds": [
        { "src": "api/json-server.js", "use": "@vercel/node" },
        { "src": "package.json", "use": "@vercel/static-build" }
    ]
}
